{
  "env": {
    "development": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"],
              "node": 6
            },
            "modules": "commonjs",
            "loose": true
          }
        ],
        "react",
        "stage-1"
      ]
    },
    "esm": {
      "presets": [
        [
          "env",
          {
            "targets": {
              "browsers": ["last 2 versions", "ie >= 9"],
              "node": 6
            },
            "modules": false,
            "loose": true
          }
        ],
        "react",
        "stage-1"
      ]
    }
  }
}
